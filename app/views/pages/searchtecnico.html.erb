<div class="container">
<h3>Resultados encontrados</h3>
<div calss="container" style="overflow-y: scroll; height:400px; overflow-x: scroll; width:1000px">
 <table>
 <thead>
    <tr>
      <th>Nombre</th>
      <th>Apellido</th>
      <th>Domicilio</th>
      <th>Telefono</th>
      <th>DNI</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
      <tr>
        <td><%= result.nombre %></td>
        <td><%= result.apellido %></td>
        <td><%= result.domicilio %></td>
        <td><%= result.telefono %></td>
        <td><%= result.dni %></td>
        <td><%= button_to  'Detalle', result, method: :get %></td>
        <td><%= button_to 'Eliminar', tecnico_path(result),method: :delete, data: { confirm: "EstÃ¡s seguro?" }  %></td>
    <% end %>
  </tbody>
</table>
</div>
</div>
