<div class="container">
  <h3>Resultados encontrados</h3>
  <div calss="container" style="overflow-y: scroll; height:400px; overflow-x: scroll; width:1000px">
     <table>
       <thead>
          <tr>
            <th>Fecha Reclamo</th>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Descripcion</th>
            <th>Nro. Certificado</th>
            <th colspan="3"></th>
          </tr>
        </thead>
      <tbody>
        <% @results.each do |result| %>
          <%if result.estado == true %>
              <tr>
                <td><%= result.fecha_queja %></td>
                <td><%= result.cliente.nombre %></td>
                <td><%= result.cliente.apellido %></td>
                <td><%= result.observacion %></td>
                <td><%= result.certificado.nro_certificado %></td>
                <td><%= link_to 'Mostrar',result,:class => "btn btn-dark"  %></td>
                <td><%= link_to 'Editar', edit_queja_path(result),:class => "btn btn-dark" %></td>
                <td><%= link_to 'Eliminar', result, method: :delete, data: { confirm: '¿Estás seguro/a?' },:class => "btn btn-dark" %></td>
              </tr>
          <% end %>
        <% end %>  
      </tbody>
    </table>
  </div>
</div>